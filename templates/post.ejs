
<% posts.forEach(function(post){ %>
<article>
 
        <h3><%= post.title %></h3>
        <h6>Written by <a href="#"><%= post.user %></a> on <time datetime="<%= post.date %>"></time>.</h6>
 
        <div class="row">
          <div class="large-6 columns">
            <%= post.body %>
          </div>
        </div>
</article>
<% }); %>